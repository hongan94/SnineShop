<h2 class="title_login">Log in</h2>
<div class="form">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.email_field :email, class:'form-control email',palceholder: 'Email', autofocus: true %>
  </div>

  <div class="field">
    <%= f.password_field :password, class: 'form-control password', palceholder: 'Password', autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" , class: 'btn_login' %>
  </div>
<% end %>
  <%= render "devise/shared/links" %>

</div>